{
  "manifest_version": 2,

  "name": "tab_state",
  "description": "A chome extension that opens and closes groups of tabs.",
  "version": "1.0",

  "permissions": [
    "tabs",
    "storage"
  ],

  "content_scripts": [
    {
      "js": [
        "js/jquery-1.11.0.min.js", 
        "js/popup.js"
      ]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}